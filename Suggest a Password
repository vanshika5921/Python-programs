import random
import string
password = [str(random.randint(1,9)) ,str(random.randint(10,99)) , random.choice(string.punctuation), chr(random.randint(65,90)),
chr(random.randint(97,122)),str(random.randint(1,9)), chr(random.randint(97,122))]
random.shuffle(password)
password = ''.join(password)
print(f'Suggested Password is : {pasword}')
